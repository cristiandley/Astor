{
	"name": "astor-agentic",
	"version": "0.2.1",
	"description": "A functional workflow and agent library for LLM applications",
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/types/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"build:types": "tsc -p tsconfig.types.json",
		"build:esm": "bun build src/astor.core.ts --outdir ./dist/esm --target node --format esm",
		"build:cjs": "bun build src/astor.core.ts --outdir ./dist/cjs --target node --format cjs",
		"build": "bun run clean && bun run build:types && bun run build:esm && bun run build:cjs",
		"test": "bun test",
		"prepublishOnly": "bun run build"
	},
	"keywords": [
		"cloudflare",
		"workers",
		"ai",
		"workflow",
		"agent",
		"chain",
		"llm",
		"functional"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cristiandley/astor.git"
	},
	"dependencies": {
		"consola": "^3.4.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "latest",
		"typescript": "^5.0.0"
	},
	"engines": {
		"node": ">=18"
	}
}
